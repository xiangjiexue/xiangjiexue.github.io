title = {Adaptive and dynamic adaptive procedures for false discovery rate control and estimation},
author = {Liang, Kun and Nettleton, Dan},
keywords = {Empirical Processes ; Martingales ; Multiple Testing ; Optional Stopping Theorem ; Simultaneous Inference ; Stopping Time},
issn = {1369-7412},
abstract = {Many methods for estimation or control of the false discovery rate (FDR) can be improved by incorporating information about , the proportion of all tested null hypotheses that are true. Estimates of are often based on the number of ‐values that exceed a threshold . We first give a finite sample proof for conservative point estimation of the FDR when the ‐parameter is fixed. Then we establish a condition under which a dynamic adaptive procedure, whose ‐parameter is determined by data, will lead to conservative ‐ and FDR estimators. We also present asymptotic results on simultaneous conservative FDR estimation and control for a class of dynamic adaptive procedures. Simulation results show that a novel dynamic adaptive procedure achieves more power through smaller estimation errors for under independence and mild dependence conditions. We conclude by discussing the connection between estimation and control of the FDR and show that several recently developed FDR control procedures can be cast in a unifying framework where the strength of the procedures can be easily evaluated.},
journal = {Journal of the Royal Statistical Society: Series B (Statistical Methodology)},
}

